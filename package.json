{
  "name": "vue-demo",
  "port": 8000,
  "private": true,
  "scripts": {
    "DEV": "NODE_ENV=DEV node app.js",
    "PRE": "NODE_ENV=PRE node app.js",
    "PRD": "NODE_ENV=PRD node app.js",
    "start": "node app.js",
    "build_pre": "rm -rf dist && npm run build_pre:client && npm run build_pre:server",
    "build_pre:client": "NODE_ENV=PRE  webpack --config ./build/webpack.client.config.js --progress --hide-modules --profile",
    "build_pre:server": "NODE_ENV=PRE webpack --config ./build/webpack.server.config.js --progress --hide-modules --profile",
    "build": "rm -rf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=PRD webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=PRD webpack --config ./build/webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
    "echarts": "3.3.0",
    "es6-promise": "4.0.5",
    "express": "4.13.3",
    "express-session": "1.13.0",
    "jquery": "1.11.0-rc1",
    "lodash": "4.17.4",
    "lru-cache": "^4.0.2",
    "memory-fs": "0.3.0",
    "moment": "^2.18.1",
    "serialize-javascript": "1.3.0",
    "serve-favicon": "2.3.2",
    "superagent": "2.2.0",
    "vue": "2.1.5",
    "vue-resource": "1.0.0",
    "vue-router": "2.0.0",
    "vue-server-renderer": "2.1.5",
    "vue-template-compiler": "2.1.5",
    "vuex": "2.0.0",
    "vuex-router-sync": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "buble": "0.15.1",
    "buble-loader": "0.4.0",
    "cors": "2.8.1",
    "cross-env": "^2.0.0",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "2.24.1",
    "http-proxy-middleware": "^0.17.4",
    "node-sass": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "purifycss-webpack": "^0.5.0",
    "sass-loader": "4.1.0",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.2",
    "url-loader": "0.5.7",
    "vue-loader": "^11.3.3",
    "webpack": "2.1.0-beta.19",
    "webpack-dev-middleware": "",
    "webpack-hot-middleware": ""
  }
}
